{
  "manifest_version": 2,
  "name": "chrome-spa-template",
  "short_name": "chrome-spa-template",
  "description": "A template useful for building single page chrome applications",
  "version": "0.0.1",
  "minimum_chrome_version": "36",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },

  "app": {
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }
}
